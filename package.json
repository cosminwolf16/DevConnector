    "express-validator": "^6.4.0",
    "gravatar": "^1.8.0",
    "http-proxy-middleware": "^1.0.3",
    "jsonwebtoken": "^9.0.0",
    "mongoose": "^5.9.5",
    "request": "^2.88.2"
